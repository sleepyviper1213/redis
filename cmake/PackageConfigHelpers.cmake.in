@PACKAGE_INIT@

include("${CMAKE_CURRENT_LIST_DIR}/redis_serverTargets.cmake")

# Define components
set(redis_server_FIND_COMPONENTS default core server)

if(NOT redis_server_FIND_COMPONENTS)
set(redis_server_FIND_COMPONENTS default)
endif()

foreach(comp ${redis_server_FIND_COMPONENTS})
if(comp STREQUAL "core")
set(redis_server_Core_FOUND TRUE)
elseif(comp STREQUAL "server")
set(redis_server_Server_FOUND TRUE)
endif()
endforeach()
